{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["DOM", "ESNext"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"]
  }